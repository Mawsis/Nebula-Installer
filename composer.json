{
  "name": "mawsis/nebula-installer",
  "description": "Nebula PHP Installer",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "bin": [
    "bin/framework"
  ],
  "require": {
    "php": ">=8.0",
    "symfony/console": "^7.2"
  },
  "scripts": {
    "post-create-project-cmd": [
      "php bin/framework new"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
